    template<unsigned int indexPara>
    using INT = MetaAD::ValueNode<MetaAD::NullTypeNode, int, indexPara>;

    template<unsigned int indexPara>
    using DOUBLE = MetaAD::ValueNode<MetaAD::NullTypeNode, double, indexPara>;

    template<unsigned int indexPara>
